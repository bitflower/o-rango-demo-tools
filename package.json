{
  "name": "@o-rango/orango-dev-tools",
  "version": "0.0.1",
  "description": "Orango dev tools components",
  "main": "dist/collection/index.js",
  "types": "dist/collection/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "browser": "dist/orango-dev-tools.js",
  "scripts": {
    "publish": "npm publish --access=public",
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "NODE_ENV=test jest --no-cache --coverage",
    "test.watch": "NODE_ENV=test --inspect jest --watch --no-cache"
  },
  "dependencies": {
    "@material/select": "^0.27.0",
    "@material/snackbar": "^0.27.0",
    "@material/toolbar": "^0.27.0",
    "@material/typography": "^0.3.0",
    "@stencil/core": "next"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "@stencil/dev-server": "latest",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^22.0.0",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^3.0.0",
    "jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-react": "^3.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/o-rango/orango-dev-tools.git"
  },
  "author": "Orango Team",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.js?$": "babel-jest",
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "transformIgnorePatterns": [
      "/node_modules/(?!@material)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  },
  "bugs": {
    "url": "https://github.com/o-rango/orango-dev-tools/issues"
  },
  "homepage": "https://github.com/o-rango/orango-dev-tools#readme",
  "keywords": [
    "web",
    "components",
    "dev",
    "tools",
    "front-end",
    "stenciljs",
    "angular",
    "react",
    "demo",
    "scenarios"
  ]
}
